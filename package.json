{
  "name": "template-api-node",
  "version": "0.0.1",
  "description": "template para api node",
  "main": "index.js",
  "scripts": {
    "clean": "npm prune",
    "check": "npm-check -s -u",
    "start": "node -r dotenv/config dist/main",
    "dev": "ts-node-dev --respawn --transpile-only -r dotenv/config --inspect=0.0.0.0:9229 src/main",
    "prebuild": "rimraf dist",
    "build": "tsc -p tsconfig.build.json",
    "lint:fix": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix --max-warnings 0",
    "test": "jest --colors --passWithNoTests --runInBand ",
    "test:unit": "npm test -- --watch -c jest-unit-config.js",
    "test:integration": "npm test -- --watch -c jest-integration-config.js",
    "test:staged": "npm test -- --findRelatedTests",
    "test:ci": "npm test -- --coverage",
    "test:ci:silent": "npm test -- --coverage --silent",
    "test:fb-api": "npm test -- -c  jest-e2e.js -- ./tests/external/facebook-api.e2e-spec.ts",
    "test:s3": "npm test -- -c  jest-e2e.js -- ./tests/external/aws-s3-file-storage.e2e-spec.ts",
    "prepare": "if [ \"$NODE_ENV\" != \"production\" ]; then husky install; fi"
  },
  "keywords": [],
  "author": {
    "name": "Antonio marcos",
    "email": "antoniomarcos.amrrds@gmail.com"
  },
  "license": "GPL-3.0-or-later",
  "devDependencies": {
    "@jest-mock/express": "^2.0.2",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.11",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/module-alias": "^2.0.4",
    "@types/node": "^20.11.8",
    "@types/supertest": "^6.0.2",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "^6.19.1",
    "eslint": "^8.56.0",
    "eslint-config-standard-with-typescript": "^43.0.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-n": "^16.6.2",
    "eslint-plugin-promise": "^6.1.1",
    "git-commit-msg-linter": "^5.0.6",
    "husky": "^8.0.0",
    "jest": "^29.7.0",
    "jest-mock": "^29.7.0",
    "jest-mock-extended": "^3.0.5",
    "lint-staged": "^15.2.0",
    "npm-check": "^6.0.1",
    "pg-mem": "^2.8.1",
    "rimraf": "^5.0.5",
    "supertest": "^6.3.4",
    "ts-jest": "^29.1.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "aws-sdk": "^2.1545.0",
    "axios": "^1.6.7",
    "dotenv": "^16.4.1",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.2",
    "module-alias": "^2.2.3",
    "pg": "^8.11.3",
    "reflect-metadata": "^0.2.1",
    "typeorm": "^0.3.20",
    "uuid": "^9.0.1"
  },
  "engines": {
    "node": "20.x"
  }
}
